<!-- Scraped Article Cards -->

<div class="container">
	{{#each Article}}
	<div class="row">
		<div class="col s12 m12 l12">
			<div class="card blue darken-2">
				<div class="card-content white-text">
					<span class="card-title">{{title}}</span>
					<p>{{description}}</p>
				</div>
				<div class="card-action">
					<a href="#">{{link}}</a>
				</div>
			</div>
		</div>
	</div>
	{{/each}}
</div>